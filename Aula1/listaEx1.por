programa
{
	
	funcao inicio()
	{
		real num1, num2
		
		
		escreva("Digite um numero: ")
		leia(num1)

		escreva("Digite outro numero: ")
		leia(num2)
 
		escreva("A soma entre ",num1," e ",num2," é igual a ",num1+num2)
	}
}
